<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingHorizontal="@dimen/_12sdp"
    tools:context=".ui.fragments.tenants.bottom.profile.TenantProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?android:attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/nav_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_ripple_white_12"
            android:clickable="true"
            android:contentDescription="@null"
            android:focusable="true"
            android:padding="@dimen/_5sdp"
            android:src="@drawable/ic_humberger_menu"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/et_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_ripple_blue_500_8"
            android:clickable="true"
            android:contentDescription="@null"
            android:drawablePadding="@dimen/_4sdp"
            android:focusable="true"
            android:fontFamily="@font/roboto_light"
            android:gravity="center"
            android:padding="@dimen/_7sdp"
            android:text="@string/edit"
            android:textColor="@color/white"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.526"
            app:drawableStartCompat="@drawable/ic_edit"
            app:drawableTint="@color/white" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_medium"
        android:text="@string/profile"
        android:textSize="@dimen/_24ssp"
        android:textColor="@color/blueDark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/tv_user_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        android:text="@string/haider_ali"
        android:textColor="@color/blackDark"
        android:textSize="@dimen/_16ssp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_light"
        android:text="@string/haiderali_gmail_com"
        android:textColor="@color/medium_gray"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_user_name" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="@dimen/_90sdp"
        android:layout_height="@dimen/_90sdp"
        android:layout_marginTop="@dimen/_12sdp"
        app:cardBackgroundColor="@color/whiteDarkInput"
        app:cardCornerRadius="@dimen/_45sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

        <ImageView
            android:id="@+id/imgProfile"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/ic_house"/>
    </androidx.cardview.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_12sdp"
            android:text="@string/phone_number"
            android:textColor="@color/blueDark"
            android:fontFamily="@font/roboto_regular"/>

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tv_phone_no"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:background="@drawable/image_bg"
            android:drawablePadding="@dimen/_12sdp"
            android:drawableStart="@drawable/ic_phone"
            android:fontFamily="@font/roboto_regular"
            android:text="@string/n_a"
            android:paddingHorizontal="@dimen/_12sdp"
            android:paddingVertical="@dimen/_16sdp"
            android:textColor="@color/blueDark"
            android:textSize="@dimen/_14ssp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_12sdp"
            android:text="@string/address_txt"
            android:textColor="@color/blueDark"
            android:fontFamily="@font/roboto_regular"/>

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tv_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:background="@drawable/image_bg"
            android:drawablePadding="@dimen/_12sdp"
            android:drawableStart="@drawable/ic_location"
            android:fontFamily="@font/roboto_regular"
            android:text="@string/n_a"
            android:paddingHorizontal="@dimen/_12sdp"
            android:paddingVertical="@dimen/_16sdp"
            android:textColor="@color/blueDark"
            android:textSize="@dimen/_14ssp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:text="@string/vehicle_information"
            android:textColor="@color/blueDark"
            android:fontFamily="@font/roboto_regular" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tv_vehicle_information"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/vehicle_information"
            android:textColor="@color/medium_gray"
            android:textSize="@dimen/_14ssp"
            android:fontFamily="@font/roboto_regular"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/bg_text_view"
            app:drawableStartCompat="@drawable/ic_person"
            android:drawablePadding="@dimen/_12sdp"
            android:drawableEnd="@drawable/ic_next"
            android:paddingHorizontal="@dimen/_12sdp"
            android:paddingVertical="@dimen/_16sdp"
            android:layout_marginTop="@dimen/_8sdp"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>